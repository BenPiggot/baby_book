<h1>Photos from "<%= @event.topic%>"</h1>

<% @media.each do |photo| %>
<div class="imgWrapper">
  <%= cl_image_tag(photo.url,
      :width => 100,
      :height => 80,
      :crop => :fill,
      :gravity => :face
        ) %>

 <p class="imgDescription"><%= link_to "Delete Photo?", baby_event_medium_path(@baby,@event,photo.id), :method => :delete %></p>


</div>
<% end %>
<div class="btn-margins-medium">
<%= link_to "Return to Timeline", baby_events_path, :class => "btn btn-info"%>
</div>